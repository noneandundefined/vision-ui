{
	"name": "@artemiik/vision-ui",
	"description": "Keep up to date with what is happening on your server using - Vision",
	"private": false,
	"version": "1.1.2",
	"type": "module",
	"main": "./dist/vision.bundle.js",
	"unpkg": "./dist/vision.bundle.js",
	"homepage": "https://github.com/noneandundefined/vision-ui",
	"repository": "git@github.com:noneandundefined/vision-ui.git",
	"files": [
		"./dist/vision.bundle.js",
		"./dist/vision.css"
	],
	"keywords": [
		"vision",
		"vision-js",
		"vision-ui",
		"monitoring",
		"metrics",
		"load-balancer",
		"stats",
		"server"
	],
	"author": "Artemiik",
	"license": "Apache-2.0 license",
	"scripts": {
		"dev": "vite",
		"test": "vitest",
		"install:prod": "set NODE_ENV=production&& pnpm install",
		"build": "tsc -b && vite build",
		"clean": "rimraf ./dist",
		"open-static": "start http://localhost:3000",
		"serve-static": "serve dist",
		"start": "npm-run-all --parallel serve-static open-static",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"preview": "vite preview"
	},
	"dependencies": {
		"axios": "^1.7.9",
		"install": "^0.13.0",
		"js-cookie": "^3.0.5",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-router-dom": "^7.1.5",
		"tailwindcss-motion": "^1.1.0",
		"vite": "^6.2.0"
	},
	"devDependencies": {
		"@babel/preset-env": "^7.26.9",
		"@babel/preset-react": "^7.26.3",
		"@babel/preset-typescript": "^7.26.0",
		"@eslint/js": "^9.19.0",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/react": "^16.2.0",
		"@testing-library/user-event": "^14.6.1",
		"@types/axios": "^0.14.4",
		"@types/js-cookie": "^3.0.6",
		"@types/react": "^19.0.8",
		"@types/react-dom": "^19.0.3",
		"@vitejs/plugin-react-swc": "^3.5.0",
		"autoprefixer": "^10.4.20",
		"axios-mock-adapter": "^2.1.0",
		"esbuild": "^0.25.0",
		"eslint": "^9.19.0",
		"eslint-plugin-react-hooks": "^5.0.0",
		"eslint-plugin-react-refresh": "^0.4.18",
		"globals": "^15.14.0",
		"husky": "^9.1.7",
		"jsdom": "^26.0.0",
		"open": "^10.1.0",
		"postcss": "^8.5.2",
		"prettier": "^3.5.1",
		"tailwindcss": "3",
		"ts-node": "^10.9.2",
		"typescript": "~5.7.2",
		"typescript-eslint": "^8.22.0",
		"vitest": "^3.0.6"
	}
}
